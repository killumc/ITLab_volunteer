<UserControl
    x:Class="test2.Views.Pages.AboutPages.About1View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:aboutPages="clr-namespace:test2.ViewModels.Pages.AboutPages"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:test2.Views.Pages.AboutPages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DataContext="{d:DesignInstance aboutPages:About1PageViewModel}"
    d:DesignHeight="693"
    d:DesignWidth="1605"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <Image Margin="0,0,54,0" Source="/Resources/Images/aboutView/page1.png" />
        <StackPanel Grid.Column="1">
            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About1View_Text1}" />
            <TextBlock
                Margin="0,21,0,21"
                Style="{StaticResource BaseTextStyle}"
                Text="{StaticResource About1View_Text2}" />
            <TextBlock
                Margin="0,0,0,21"
                FontFamily="{StaticResource TitleFont}"
                Foreground="{StaticResource RedBrush}"
                Style="{StaticResource BaseTextStyle}"
                Text="{StaticResource About1View_Title}" />

            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About1View_list1}" />
            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About1View_list2}" />
            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About1View_list3}" />
            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About1View_list4}" />
            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About1View_list5}" />

        </StackPanel>
    </Grid>
</UserControl>
