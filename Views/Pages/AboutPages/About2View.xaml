<UserControl
    x:Class="test2.Views.Pages.AboutPages.About2View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:aboutPages="clr-namespace:test2.ViewModels.Pages.AboutPages"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:test2.Views.Pages.AboutPages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DataContext="{d:DesignInstance aboutPages:About2PageViewModel}"
    d:DesignHeight="693"
    d:DesignWidth="1605"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <StackPanel VerticalAlignment="Center">
            <TextBlock
                Width="347"
                Height="61"
                Style="{StaticResource TitleTextType2}"
                Text="{StaticResource About2View_Title1}">
                <TextBlock.Background>
                    <ImageBrush ImageSource="/Resources/Images/VectorBlue.png" />
                </TextBlock.Background>
            </TextBlock>
            <TextBlock
                Width="537"
                Margin="0,24,0,94"
                Style="{StaticResource BaseTextStyle}"
                Text="{StaticResource About2View_Text1}"
                TextAlignment="Center" />
            <TextBlock
                Width="274"
                Height="60"
                Margin="0,0,0,24"
                Style="{StaticResource TitleTextType2}"
                Text="{StaticResource About2View_Title2}">
                <TextBlock.Background>
                    <ImageBrush ImageSource="/Resources/Images/VectorRed.png" />
                </TextBlock.Background>
            </TextBlock>
            <TextBlock
                Width="757.5"
                Style="{StaticResource BaseTextStyle}"
                Text="{StaticResource About2View_Text2}"
                TextAlignment="Center" />
        </StackPanel>
        <StackPanel Grid.Column="1" VerticalAlignment="Center">
            <TextBlock
                Margin="0,0,0,36"
                FontSize="40"
                Style="{StaticResource TitleText}"
                Text="{StaticResource About2View_Title3}" />
            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About2View_Text3_1}" />
            <TextBlock
                Margin="0,21,0,21"
                Style="{StaticResource BaseTextStyle}"
                Text="{StaticResource About2View_Text3_2}" />
            <TextBlock Style="{StaticResource BaseTextStyle}" Text="{StaticResource About2View_Text3_3}" />

        </StackPanel>
    </Grid>
</UserControl>
