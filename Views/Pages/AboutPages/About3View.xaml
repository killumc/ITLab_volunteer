                <Run FontFamily="{StaticResource TitleFont}" Text="{StaticResource About3View_subTitle14}" />
                <Run Text="{StaticResource About3View_subText14}" />
            </TextBlock>



        </StackPanel>
        <StackPanel Grid.Column="1" Width="607">
            <TextBlock
                Margin="0,0,0,36"
                FontSize="40"
                Style="{StaticResource TitleText}"
                Text="{StaticResource About3View_Title2}" />
            <Button Content="{StaticResource About3View_Btn1}" Style="{StaticResource DocBtnStyle}" />
