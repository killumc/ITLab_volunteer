<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:aboutPages="clr-namespace:test2.ViewModels.Pages.AboutPages"
    xmlns:aboutPages1="clr-namespace:test2.Views.Pages.AboutPages"
    xmlns:pages="clr-namespace:test2.ViewModels.Pages"
    xmlns:pages1="clr-namespace:test2.Views.Pages">
    <DataTemplate DataType="{x:Type pages:MainPageViewModel}">
        <pages1:MainView DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pages:ProjectPageViewModel}">
        <pages1:ProjectView DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pages:SelectedProjectPageViewModel}">
        <pages1:SelectedProjectView DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pages:AboutPageViewModel}">
        <pages1:AboutView DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pages:DonatePageViewModel}">
        <pages1:DonateView DataContext="{Binding}" />
    </DataTemplate>


    <DataTemplate DataType="{x:Type aboutPages:About1PageViewModel}">
        <aboutPages1:About1View DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type aboutPages:About2PageViewModel}">
        <aboutPages1:About2View DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type aboutPages:About3PageViewModel}">
        <aboutPages1:About3View DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type aboutPages:About4PageViewModel}">
        <aboutPages1:About4View DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type aboutPages:About5PageViewModel}">
        <aboutPages1:About5View DataContext="{Binding}" />
    </DataTemplate>
</ResourceDictionary>