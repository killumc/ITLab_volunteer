<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Здесь цвета даны для примера, замените на те, которые есть в макете  -->
    <Color x:Key="BlackColor">#292929</Color>
    <Color x:Key="YellowColor">#FFBE02</Color>
    <Color x:Key="OrangeColor">#FE6A02</Color>
    <Color x:Key="AlmostWhite">#ECF5EF</Color>

    <Color x:Key="BlackTestColor">#1E1E1E</Color>
    <Color x:Key="RedTestColor">#E31E24</Color>
    <Color x:Key="BlueTestColor">#2683F1</Color>
    <Color x:Key="GreenCardColor">#4D9E93</Color>
    <Color x:Key="GreenCardBorderColor">#165C53</Color>
    <Color x:Key="RedCardColor">#F45952</Color>
    <Color x:Key="RedCardBorderColor">#64110E</Color>

    <Color x:Key="RoundedBtnColor">#12529E</Color>
    <Color x:Key="DonateBorderColor">#33877C</Color>
    <Color x:Key="GreyTestColor">#707070</Color>

    <Color x:Key="DarkerRedColor">#CC261E</Color>
    <Color x:Key="TransparentRedColor">#10F45952</Color>
    <Color x:Key="DarkerGreenColor">#24B03E</Color>


    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource BlackColor}" />
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource YellowColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource OrangeColor}" />
    <SolidColorBrush x:Key="ThirdBrush" Color="{StaticResource AlmostWhite}" />
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource BlackTestColor}" />
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedTestColor}" />
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource BlueTestColor}" />
    <SolidColorBrush x:Key="GreenCardBrush" Color="{StaticResource GreenCardColor}" />
    <SolidColorBrush x:Key="GreenCardBorderBrush" Color="{StaticResource GreenCardBorderColor}" />
    <SolidColorBrush x:Key="RedCardBrush" Color="{StaticResource RedCardColor}" />
    <SolidColorBrush x:Key="RedCardBorderBrush" Color="{StaticResource RedCardBorderColor}" />
    <SolidColorBrush x:Key="RoundedBtnBrush" Color="{StaticResource RoundedBtnColor}" />
    <SolidColorBrush x:Key="DonateBorderBrush" Color="{StaticResource DonateBorderColor}" />
    <SolidColorBrush x:Key="GreyBrush" Color="{StaticResource GreyTestColor}" />
    <SolidColorBrush x:Key="DarkerRedBrush" Color="{StaticResource DarkerRedColor}" />
    <SolidColorBrush x:Key="DarkerGreenBrush" Color="{StaticResource DarkerGreenColor}" />
    <SolidColorBrush x:Key="TransparentRedBrush" Color="{StaticResource TransparentRedColor}" />
</ResourceDictionary>